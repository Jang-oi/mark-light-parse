{
  "name": "mark-light-parse",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "main": "electron/main.js",
  "homepage": ".",
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none vite\" \"wait-on http://localhost:3000 && electron .\"",
    "deploy": "vite build && electron-builder build --win --publish always && rimraf dist",
    "build": "vite build && electron-builder",
    "build:win": "vite build && electron-builder --win --x64"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jang-oi/mark-light-parse.git"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-table": "^8.20.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "electron-is-dev": "^3.0.1",
    "electron-localshortcut": "^3.2.1",
    "electron-updater": "^6.2.1",
    "embla-carousel-react": "^8.3.0",
    "lucide-react": "^0.427.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "rimraf": "^6.0.1",
    "tailwind-merge": "^2.5.0",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^9.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.41",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0",
    "wait-on": "^7.2.0"
  },
  "author": {
    "name": "Mark-Light ManageMent"
  },
  "build": {
    "appId": "com.marklight.parse.app",
    "productName": "Mark-Light",
    "files": [
      "electron/*",
      "build/**/*"
    ],
    "asar": false,
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "createDesktopShortcut": true,
      "shortcutName": "Mark-Light",
      "artifactName": "Mark-Light ${version}.${ext}",
      "uninstallDisplayName": "Mark-Light"
    },
    "publish": {
      "provider": "github"
    }
  }
}
